!scrollbar style - rxvt (default), plain (most compact), next, false, or xterm
URxvt.scrollBar: false

! FONT SETTINGS
!{{{
URxvt*font          : xft:lemon:style=regular:antialias=false:pixelsize=10
URxvt*boldFont:
URxvt*letterSpace   : 0

Xft.dpi             : 98

Xft.autohint        : 0
Xft.lcdfilter       : lcddefault
Xft.hintstyle       : hintfull
Xft.hinting         : 1
Xft.antialiasing    : true
Xft.rgba            : rgb
!}}}

! KEY COMMANDS
URxvt.keysym.Shift-Up     : command:\033]720;1\007
URxvt.keysym.Shift-Down   : command:\033]721;1\007

URxvt.urlLauncher         : firefox-developer
URxvt.visualBell          : false
URxvt.iso14755            : false

! COPY PASTE
URxvt.perl-ext-common     : clipboard

URxvt.clipboard.copycmd   : clipit
URxvt.clipboard.pastecmd  : clipit -c

URxvt.keysym.C-C          : perl:clipboard:copy
URxvt.keysym.C-V          : perl:clipboard:paste

URxvt.clipboard.autocopy  : false


! General Settings
URxvt*termName            : xterm-256color
URxvt*loginShell          : true
!URxvt*skipScroll          : true
!URxvt*geometry            : 90x65
URxvt*scrollTtyOutput     : false
URxvt*scrollTtyKeypress   : true
URxvt*scrollWithBuffer    : false
UURxvt*saveLines          : 1500
URxvt*urgentOnBell        : false
URxvt*cursorBlink         : 0

URxvt*skipBuiltinGlyphs   : true
!URxvt*transparent        : false
!URxvt*inheritPixmap      : true

! COLORS
!!! Phallus Custom Colorscheme
!! black
URxvt*color15          : #66666A
URxvt*color7           : #30303A

!!! red
URxvt*color1           : #DA637E
URxvt*color9           : #F0A3A3

!! yellow/orange
URxvt*color3           : #AB93B9
URxvt*color11          : #CBB6D7

!! purple
URxvt*color5           : #AC8AC1
URxvt*color13          : #D2BBE4

!! blue
URxvt*color4           : #7787A3
URxvt*color12          : #A8C9EB

!! green
URxvt*color2           : #B1B792
URxvt*color10          : #C7E6B8

!! cyan
URxvt*color6           : #63918B
URxvt*color14          : #92B5B0

!! white
URxvt*color0           : #B1B8C3
URxvt*color8           : #CDCDCD

URxvt*background       : #20202F
URxvt*foreground       : #BDBDC3
!URxvt*tint             : #7787A3
URxvt*cursorColor      : #F0A3A3
URxvt*internalBorder   : 10
URxvt*borderColor      : #20202F


