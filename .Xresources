!scrollbar style - rxvt (default), plain (most compact), next, false, or xterm
*scrollBar: false

! FONT SETTINGS
!{{{
*font          : xft:lemon:style=regular:antialias=false:pixelsize=10
*boldFont:
*letterSpace   : 0

Xft.dpi             : 96

Xft.autohint        : 0
Xft.lcdfilter       : lcddefault
Xft.hintstyle       : hintfull
Xft.hinting         : 1
Xft.antialiasing    : true
Xft.rgba            : rgb
!}}}

! KEY COMMANDS
URxvt.keysym.Shift-Up     : command:\033]720;1\007
URxvt.keysym.Shift-Down   : command:\033]721;1\007

URxvt.urlLauncher         : firefox-developer
URxvt.visualBell          : false
URxvt.iso14755            : false

! COPY PASTE
URxvt.perl-ext-common     : clipboard

URxvt.clipboard.copycmd   : clipit
URxvt.clipboard.pastecmd  : clipit -c

URxvt.keysym.C-C          : perl:clipboard:copy
URxvt.keysym.C-V          : perl:clipboard:paste

URxvt.clipboard.autocopy  : false


! General Settings
URxvt*termName            : xterm-256color
URxvt*loginShell          : true
URxvt*skipScroll          : false
!URxvt*geometry            : 90x65
URxvt*scrollTtyOutput     : false
URxvt*scrollTtyKeypress   : true
URxvt*scrollWithBuffer    : false
URxvt*buffered            : false
UURxvt*saveLines          : 100
URxvt*urgentOnBell        : false
URxvt*cursorBlink         : 0

URxvt*skipBuiltinGlyphs   : true
URxvt*transparent        : false
!URxvt*inheritPixmap      : true

! COLORS
!!! Phallus Custom Colorscheme
!! black
*color15          : #66666A
*color7           : #34444D

!!! red
*color1           : #DA637E
*color9           : #F0A3A3

!! yellow/orange
*color3           : #AB93B9
*color11          : #CBB6D7

!! purple
*color5           : #AC8AC1
*color13          : #D2BBE4

!! blue
*color4           : #6F8BBD
*color12          : #8CB4FA

!! green
*color2           : #B1B792
*color10          : #C7E6B8

!! cyan
*color6           : #63918B
*color14          : #92B5B0

!! white
*color0           : #DEE7F5
*color8           : #F7F7F7

*background       : #263238
!*background       : #20202F
*foreground       : #DEE7F5
*cursorColor      : #00BCD4
*internalBorder   : 10
*borderColor      : #263238


