let mapleader=","
nnoremap ; :
nnoremap <S-j> :back<CR>
nnoremap <S-k> :forward<CR>
nnoremap <S-h> :tabprevious<CR>
nnoremap <S-l> :tabnext<CR>
nnoremap x :tabclose<CR>
nnoremap <S-x> :undo<CR>
nnoremap <Leader>sov :source ~/.vimperatorrc<CR>
nnoremap <Leader>/ :nohlsearch<CR>
nnoremap <Leader>> ]]
nnoremap <Leader>< [[
nnoremap <Leader>t :set gui=invtabs<CR>
nnoremap <Leader>us :set invusermode<CR>
inoremap jk <ESC>
inoremap <C-e> <C-i>

set defsearch=google
set ignorecase
set smartcase
set suggestengines=google
set focuscontent
set hintchars=fjdkslaghrueiwovncm
set hinttimeout=500
set followhints=0
set animations
set gui=none
set status=tabcount,input,location,bookmark,history,position
set complete=slShbt
set verbose=5
set editor='gvim\ -f'

let smoozie_scroll_interval="100"

hi clear
hi HintElem background-color: #1a1a1a; color: white;
hi HintActive background-color: #ace594;
hi Hint font-family: monospace; font-size: 10px; font-weight: bold; text-transform: uppercase; color: white; background-color: #5B92FA; border-color: ButtonShadow; border-width: 0px; border-style: solid; padding: 0px 3px 0px 3px; text-shadow: 0 2px rgba(0,0,0,.3);
hi CmdLine background-color: #1a1a1a; color: #f1f3f4; font-weight: 500;
hi CompGroup background-color: #1a1a1a; color: #f1f3f4; font-weight: 500;
hi CompItem background-color: #1a1a1a; color: #f1f3f4; font-weight: 500;
hi CompItem[selected] color: #2eb9ff !important;
hi CompTitle background-color: #1a1a1a; color: #a1ff7a; font-weight: 800; text-shadow: 0 2px rgba(0,0,0,.3); border-width: 0px;
hi ErrorMsg background-color: #ff2e4c; color: #f1f3f4; font-weight: 800;
hi Filter background-color: none; color: #2eb9ff;
hi StatusLine background-color: #1a1a1a; color: #ace594; border-color: #1a1a1a;
hi URL color: #ace594;
hi Normal background-color: #1a1a1a; color: #ace594; border-top: 1px solid #1a1a1a;
hi LineNr color: #b3bdc2;

" vim:filetype=vim
