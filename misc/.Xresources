*scrollBar: false

! FONT SETTINGS
!{{{

! URxvt*font      : xft:lemon:style=regular:antialias=false:pixelsize=10
URxvt*font        : -*-terminesspowerline-*-*-*-*-12-*-*-*-*-*-*-*
URxvt*boldFont    : -*-terminesspowerline-bold-*-*-*-12-*-*-*-*-*-*-*
URxvt*letterSpace : 0

! :Bind C-0, C-+ and C-= to activate small, medium, and big font size resp.
URxvt.keysym.C-0: command:\033]710;-*-terminesspowerline-*-*-*-*-12-*-*-*-*-*-*-*\007
URxvt.keysym.C-minus: command:\033]710;xft:lemon:style=regular:antialias=false;pixelsize=10\007
URxvt.keysym.C-equal: command:\033]710;-*-terminesspowerline-*-*-*-*-14-*-*-*-*-*-*-*\007

!}}}

! KEY COMMANDS
! {{{

URxvt.keysym.Shift-Up     : command:\033]720;1\007
URxvt.keysym.Shift-Down   : command:\033]721;1\007

URxvt.urlLauncher         : firefox-developer
URxvt.visualBell          : false
URxvt.iso14755            : false

! COPY PASTE
URxvt.perl-ext-common     : clipboard

URxvt.clipboard.copycmd   : clipit
URxvt.clipboard.pastecmd  : clipit -c

URxvt.keysym.C-C          : perl:clipboard:copy
URxvt.keysym.C-V          : perl:clipboard:paste

URxvt.clipboard.autocopy  : false

! }}}

! URxvt Settings
! {{{

URxvt*termName          : xterm-256color
URxvt*loginShell        : true
URxvt*skipScroll        : false
URxvt*scrollTtyOutput   : false
URxvt*scrollTtyKeypress : true
URxvt*scrollWithBuffer  : false
URxvt*buffered          : true
!URxvt*buffered          : false
UURxvt*saveLines        : 1500
URxvt*urgentOnBell      : false
URxvt*cursorBlink       : 0

URxvt*skipBuiltinGlyphs : false
URxvt*transparent       : false
!URxvt*inheritPixmap    : true

!}}}

